<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="compose-form" name="composeForm" method="post">    <! -- action="/compose" -->
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text"  name="postTitle">
    <label>Post</label>
    <textarea class="form-control" name="postBody"  rows="5" cols="30"></textarea>

  </div>
  <button class="btn btn-primary submit-content-btn" type="submit" >Publish</button>

</form>

